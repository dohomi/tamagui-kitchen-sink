{
  "name": "tamagui-extras",
  "license": "MIT",
  "author": "Dominic Garms",
  "version": "0.12.0",
  "repository": "https://github.com/dohomi/tamagui-kitchen-sink",
  "homepage": "https://tamagui-extras.vercel.app",
  "sideEffects": [
    "*.css"
  ],
  "keywords": [
    "react",
    "react-native",
    "@tamagui/core",
    "tamagui",
    "react-hook-form"
  ],
  "types": "./types/index",
  "main": "src/index.ts",
  "files": [
    "types/**/*",
    "link/**/*",
    "youtube/**/*",
    "richText/**/*",
    "video/**/*",
    "form/**/*",
    "file/**/*"
  ],
  "scripts": {
    "build": "rm -rf dist types && tamagui-build src/index.tsx",
    "watch": "tamagui-build src/index.tsx --watch",
    "check-types": "tsc --noEmit",
    "build-sb": "storybook build -s public -o public",
    "sb": "storybook dev -p 6006 -s ./public",
    "release-minor": "yarn build && npx release minor",
    "release-patch": "yarn build && npx release patch",
    "release-major": "yarn build && npx release major",
    "npmPublish": "npm publish"
  },
  "dependencies": {
    "@datepicker-react/hooks": "^2.8.4",
    "@siposdani87/expo-rich-text-editor": "^0.9.2",
    "@tiptap/pm": "2.0.0-beta.220",
    "@tiptap/react": "2.0.0-beta.220",
    "@tiptap/starter-kit": "2.0.0-beta.220",
    "expo-av": "^13.2.1",
    "expo-document-picker": "^11.2.2",
    "react-hook-form": "^7.43.9",
    "react-native-svg": "^13.9.0",
    "react-native-webview": "^11.26.1"
  },
  "devDependencies": {
    "@storybook/addon-actions": "^7.0.2",
    "@storybook/addon-essentials": "^7.0.2",
    "@storybook/addon-links": "^7.0.2",
    "@storybook/addon-react-native-web": "^0.0.20",
    "@storybook/nextjs": "^7.0.2",
    "@storybook/react": "^7.0.2",
    "@tamagui/animations-react-native": "1.11.1",
    "@tamagui/build": "1.11.1",
    "@tamagui/font-inter": "1.11.1",
    "@tamagui/react-native-media-driver": "1.11.1",
    "@tamagui/shorthands": "1.11.1",
    "@tamagui/themes": "1.11.1",
    "prettier": "^2.8.7",
    "solito": "^3.0.0",
    "storybook": "^7.0.2",
    "tamagui": "1.11.1",
    "tamagui-phosphor-icons": "0.2.0"
  },
  "peerDependencies": {
    "solito": ">=3.0.0",
    "tamagui": "1.11.1",
    "tamagui-phosphor-icons": "0.2.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
